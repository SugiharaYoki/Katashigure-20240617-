execute if score stageSeconds Azr_system matches 39..51 run tag @a[tag=azrPlayer] add azrUpdateSpawnPoint
execute if score stageSeconds Azr_system matches 39..42 run scoreboard players reset $Doppel Azr_system 
execute if score stageSeconds Azr_system matches 32..34 run bossbar remove azr:progress_bar_special
execute if score stageSeconds Azr_system matches 39..42 run bossbar add azr:progress_bar_special "Stage 13"
execute if score stageSeconds Azr_system matches 39..42 run bossbar set azr:progress_bar_special color blue
execute if score stageSeconds Azr_system matches 39..42 run bossbar set azr:progress_bar_special players @a[tag=azrPlayer]
execute if score stageSeconds Azr_system matches 40..42 store result bossbar azr:progress_bar_special max run scoreboard players get mobCount Azr_system
execute if score stageSeconds Azr_system matches 40.. store result bossbar azr:progress_bar_special value run scoreboard players get mobCount Azr_system
execute if score stageSeconds Azr_system matches 319.. run bossbar remove azr:progress_bar_special
execute if score stageSeconds Azr_system matches 319 run scoreboard players reset $Doppel Azr_system 
#
#mainside:none
execute if score stageSeconds Azr_system matches 10 run kill @e[tag=AzrielBossFA]

execute if score stageSeconds Azr_system matches 10.. as @e[name="僵尸盔甲匠"] at @s run function skyblock:azr/effects/zombie_armorer
execute if score stageSeconds Azr_system matches 10.. as @e[name="僵尸牧师"] at @s if score #rng8 Azr_system matches 1 run function skyblock:azr/effects/zombie_cleric
execute if score stageSeconds Azr_system matches 10.. as @e[name="僵尸邪教徒"] at @s run function skyblock:azr/effects/zombie_cartographer
execute if score stageSeconds Azr_system matches 10 run summon item_frame -79929 41 -104 {Item:{id:"minecraft:glistering_melon_slice",count:1b},Facing:5}


execute if score stageSeconds Azr_system matches 10 run advancement grant @a[tag=azrPlayer] only skyblock:azr/azr_stage13_event0
execute if score stageSeconds Azr_system matches 1 as @a[tag=azrPlayer] at @s run playsound music.nether.crimson_forest ambient @s ~ ~ ~ 8 1.0
execute if score stageSeconds Azr_system matches 3 as @a[tag=azrPlayer] at @s run playsound event.raid.horn ambient @s ~50 ~ ~-50 483 0.9

execute if score stageSeconds Azr_system matches 10 as @r at @s as @n[tag=AzrielMob,type=piglin] at @s run playsound entity.piglin.angry master @a ~ ~ ~ 4 0.9
execute if score stageSeconds Azr_system matches 10 run tellraw @a[tag=azrPlayer] {"text":"猪灵？：","color":"aqua"}
execute if score stageSeconds Azr_system matches 10 run tellraw @a[tag=azrPlayer] {"text":"“找到了，人类…… 可以吃的人类！”","color":"dark_red"}

execute if score stageSeconds Azr_system matches 50 as @e[sort=random,limit=1,tag=AzrielMob,type=piglin] at @s run playsound entity.piglin.angry master @a ~ ~ ~ 4 0.9
execute if score stageSeconds Azr_system matches 50 run tellraw @a[tag=azrPlayer] {"text":"敌意极强的猪灵：","color":"aqua"}
execute if score stageSeconds Azr_system matches 50 run tellraw @a[tag=azrPlayer] {"text":"“是人类的亡灵？！有实体的灵魂，饱含魔力的美餐啊！”","color":"dark_red"}

